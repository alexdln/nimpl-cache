{
  "name": "nimpl-cache",
  "version": "1.0.0",
  "description": "Repository for implementing caching solutions in next.js",
  "scripts": {
    "nimpl:cache-adapter": "pnpm --filter @nimpl/cache-adapter",
    "nimpl:cache-in-memory": "pnpm --filter @nimpl/cache-in-memory",
    "lint": "eslint \".\"",
    "eslint": "eslint",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alexdln/nimpl-cache.git"
  },
  "author": {
    "name": "Alex Savelyev",
    "email": "dev@alexdln.com",
    "url": "https://github.com/alexdln/"
  },
  "devDependencies": {
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "husky": "9.1.7",
    "prettier": "3.7.4",
    "typescript-eslint": "8.48.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": ["eslint"]
  },
  "resolutions": {
    "@nimpl/cache-widget>@nimpl/cache-redis": "workspace:*"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
