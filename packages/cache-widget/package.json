{
  "name": "@nimpl/cache-widget",
  "version": "0.0.1",
  "description": "",
  "keywords": [
    "next",
    "nextjs",
    "react",
    "cache",
    "cache-handler"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./widget": {
      "types": "./dist/cjs/widget.d.ts",
      "import": "./dist/esm/widget.js",
      "require": "./dist/cjs/widget.js",
      "default": "./dist/cjs/widget.js"
    },
    "./route": {
      "types": "./dist/cjs/route.d.ts",
      "import": "./dist/esm/route.js",
      "require": "./dist/cjs/route.js",
      "default": "./dist/cjs/route.js"
    },
    "./styles.css": {
      "default": "./dist/cjs/styles.css"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/alexdln/nimpl-cache.git"
  },
  "author": {
    "name": "Alex Savelyev",
    "email": "dev@alexdln.com",
    "url": "https://github.com/alexdln/"
  },
  "devDependencies": {
    "@nimpl/cache-redis": "0.1.0",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "react": "19.2.1",
    "rollup": "4.53.3",
    "rollup-plugin-scss": "4.0.1",
    "rollup-preserve-directives": "1.1.3",
    "sass": "1.95.0",
    "tslib": "2.8.1",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "@nimpl/cache-redis": ">=0.0.1",
    "react": ">=18.0.0"
  },
  "license": "MIT"
}